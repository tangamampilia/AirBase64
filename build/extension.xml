<extension xmlns="http://ns.adobe.com/air/extension/3.1">
    <id>net.tangamampilia.Base64Encoder</id>
    <versionNumber>1.0</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libBase64Encoder.a</nativeLibrary>
                <initializer>Base64EncoderInitializer</initializer>
                <finalizer>Base64EncoderFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default"> 
            <applicationDeployment/> 
        </platform>
    </platforms>
</extension>